{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "eslint.validate": ["javascript", "typescript", "typescriptreact"],
    "workbench.colorCustomizations": {
        "tab.activeBackground": "#0b403d",
        "tab.hoverBackground": "#0b403d",
        "activityBar.activeBackground": "#213161",
        "activityBar.background": "#213161",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#be4160",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#213161",
        "statusBar.background": "#141e3b",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#213161",
        "statusBarItem.remoteBackground": "#141e3b",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#141e3b",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#141e3b99",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.color": "#141e3b",
    "cSpell.words": ["fecafil", "OIDC", "Provida"]
}
